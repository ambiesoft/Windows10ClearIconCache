<Window x:Class="Windows10ClearIconCache.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows10ClearIconCache"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="txtLog" RenderTransformOrigin="0.826,2.175" Margin="10,10,10,87" AcceptsReturn="True" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>
        <Button Content="Clear Icon Cache" RenderTransformOrigin="1.955,14.394" Margin="0,0,10,10" Click="Button_Click" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="124"/>
        <CheckBox x:Name="chkIconCacheDB" Content="IconCache.db" HorizontalAlignment="Left" Margin="10,0,0,67" Height="15" VerticalAlignment="Bottom"/>
        <CheckBox x:Name="chkIconcacheALLDB" Content="iconcache_ALL_db" HorizontalAlignment="Left" Margin="10,0,0,47" Height="15" VerticalAlignment="Bottom"/>
        <CheckBox x:Name="chkThumbcacheALLDB" Content="thumbcache_ALL_db" HorizontalAlignment="Left" Margin="10,0,0,27" Height="15" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
